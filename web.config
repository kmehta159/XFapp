<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appSettings>
        <!-- Required settings -->
        <add key="WSGI_HANDLER" value="django.core.wsgi.get_wsgi_application()" />
        <!-- Your django path -->
        <add key="PYTHONPATH" value="C:\pyvirtualenv\XFapp" />
        <!-- Your djangoname.settings -->
        <add key="DJANGO_SETTINGS_MODULE" value="base.settings" />
    </appSettings>
    <system.webServer>
        <handlers>
            <add name="testiis" path="*" verb="*" modules="FastCgiModule" scriptProcessor="C:\pyvirtualenv\Seahorse\Scripts\python.exe|C:\pyvirtualenv\Seahorse\Lib\site-packages\wfastcgi.py" resourceType="Unspecified" />
        </handlers>
    </system.webServer>
</configuration>
